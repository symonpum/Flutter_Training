{
  "name": "food-delivery-api",
  "version": "1.0.0",
  "description": "Mock API server for food delivery app",
  "main": "server.js",
  "scripts": {
    "start": "json-server --watch db.json --port 3001 --host 0.0.0.0",
    "dev": "json-server --watch db.json --port 3001 --host 0.0.0.0 --delay 500",
    "reset": "cp db-backup.json db.json && npm start"
  },
  "keywords": [
    "json-server",
    "mock-api",
    "food-delivery"
  ],
  "author": "Food Delivery Team",
  "license": "MIT",
  "dependencies": {
    "json-server": "^0.17.4"
  }
}
